<Window x:Class="Fines2Wpf.Windows.AlumnoComision.DesactivarAlumnosNoCalificados.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fines2Wpf.Windows.AlumnoComision.DesactivarAlumnosNoCalificados"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <StackPanel>
        <DataGrid 
            Name="alumnosParaActivarDataGrid" 
            AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="DNI" Binding="{Binding persona__numero_documento}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
        <DataGrid 
            Name="alumnosParaDesactivarDataGrid" 
            AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="DNI" Binding="{Binding persona__numero_documento}"></DataGridTextColumn>
            </DataGrid.Columns>        
        </DataGrid>
    </StackPanel>
</Window>
